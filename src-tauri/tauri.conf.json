{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "jrpce",
  "version": "0.4.0",
  "identifier": "com.nnfz.jrpce",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "jrpce",
        "width": 950,
        "height": 700,
        "minWidth": 600,
        "minHeight": 450,
        "decorations": false
      }
    ],
    "security": {
      "csp": null,
      "capabilities": [
        "default",
        "fs:config"
      ]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
